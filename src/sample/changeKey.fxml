<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="186.0" prefWidth="365.0" styleClass="changekeymainpane" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChangeKeyController">
   <children>
      <Label fx:id="changekeylabel" layoutX="119.0" layoutY="14.0" text="Change Key" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="24.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="87.0" text="Enter Key :" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <PasswordField fx:id="keyField" layoutX="102.0" layoutY="87.0" onKeyTyped="#onKeyTyped" prefHeight="25.0" prefWidth="237.0" />
      <Label layoutX="150.0" layoutY="69.0" text="Key must be 16 characters." textFill="WHITE" />
      <CheckBox fx:id="showKey" layoutX="102.0" layoutY="112.0" mnemonicParsing="false" onAction="#onShowKey" text="Show Key" textFill="WHITE">
         <padding>
            <Insets top="5.0" />
         </padding>
      </CheckBox>
      <Button fx:id="doneBtn" layoutX="253.0" layoutY="136.0" mnemonicParsing="false" onAction="#onDone" prefHeight="35.0" prefWidth="85.0" styleClass="cwBtn" text="Done">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
